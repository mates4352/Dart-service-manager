header.header.header--bg
	.conteiner

			.header__top

				.header__top-col
					img.header__logg(src=way+"/header/icon.png", alt="")
					.header__wrapper
						p.header__logo-text Dart
						h4.header__logo-title service manager

				nav.header__nav.nav
					ul.nav__list
						li.nav__items
							a.nav__link(href="#") Home
							a.nav__link(href="#") Service
							a.nav__link(href="#") Extension
							a.nav__link(href="#") Pricing
							a.nav__link(href="#") Help

				.header__top-col
					button.header__bth sign up

			.header__bottom

				.header__description
					h1.header__title lorem Ipsum Dolor sit amet
					p.text.header__text Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamclaboris nisi ut aliquip ex ea commodo consequat.
					button.bth Buy now
					button.bth.header__bth-right Try for free

				.header__media.media
					.media-bg
					.media-controls
						.media__button
							button.media__bth.media__bth--controls
							button.media__bth.media__bth--slowDown
							button.media__bth.media__bth--normalSpeed
							button.media__bth.media__bth--speedUp
							button.media__bth.media__bth--volume
							input.media__radio(type="range" value="100" max="100" step="1")
							p.media__sound 0
						.media__conteinerBar
							p.media__durationBar-time <span class="hour">0</span>:<span class="minute">0</span>:<span class="second">0</span>
							.media__durationBar
					video.media__video( src="assets/vidao.mp4" type="video/mp4" onTimeUpdate="progressUpdate()") 
					button.media-bth
				
